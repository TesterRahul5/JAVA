class RevArr
{
  public static void main(String args[])
  {
    int list[] = { 1,5,2,9};
      int temp;
    for(int i=0;i<list.length;i++)
    {
       if(list[i] > list[i+1])
       {
           temp = list[i+1];
           list[i+1] = list[i];
           list[i] = temp;
       }
       System.out.println(list[i]);
    }
  }
}
