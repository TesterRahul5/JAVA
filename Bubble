class Bubble
{
  public static void main(String args[])
  {
    Scanner sc = new Scanner(System.in);
    int arr[] = { 7,3,8,1};
    int temp;
      for(int i=0;i<arr.length-1;i++)
      {
        for(j=0;j<arr.length-1;j++)
        {
           if(arr[j] > arr[j+1])
           {
              temp = arr[j];
              arr[j] = arr[j+1];
              arr[j+1] = temp;
           }
       System.out.println(arr[i]);
       }
   }
}
            
